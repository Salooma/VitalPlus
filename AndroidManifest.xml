    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_newapp_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.vital.TabManager"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
        android:name="SecondActivity"
        android:label="Second Activity"
        android:parentActivityName="com.example.vital.TabManager">
		</activity>
		
        <activity android:name=".HRFragment"  ></activity>
        <activity android:name=".OxygenFragment"  ></activity> 
        <activity android:name=".TempFragment"  ></activity>
        <activity android:name=".TabsPagerAdapter"  ></activity>
        
        
    </application>
</manifest>